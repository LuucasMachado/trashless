<div id="map"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {
        lat: <%= @cooperative.lat %>,
        lng: <%= @cooperative.lng %>
      },
      zoom: 12
    });
    <% @removal_orders.each_with_index do |location, index| %>
      new google.maps.Marker({
        position: {
          lat: <%= location.latitude %>,
          lng: <%= location.longitude %>},
          map: map,
          zIndex: <%= index + 1 %>
          })
    <% end %>
  }
</script>
