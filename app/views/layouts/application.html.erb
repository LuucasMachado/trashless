<!DOCTYPE html>
<html>
  <head>
    <title>Trashless</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "shared/nav" %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
     <script src="https://maps.googleapis.com/maps/api/js?key=<%= Rails.configuration.google_maps['key_api']%>&callback=initMap"
      async defer></script>
  </body>
</html>
